    </xs:sequence>
  </xs:complexType>
  <xs:element name="ArrayOfDataRecord" nillable="true" type="tns:ArrayOfDataRecord" />
  <xs:complexType name="DataRecord">
    <xs:sequence>
      <xs:element minOccurs="0" name="Id" type="xs:unsignedInt" />
